      "font-size": {
        "REM": {
          "$type": "fontSizes",
          "$value": "16",
          "$description": "The base-size of all font sizes. The default is 16(px), so it relates to 1rem = 16(px). If you change it, it will change all font-related tokens."
        },
        "typescale": {
          "scale": {
            "$type": "fontSizes",
            "$value": "1.25",
            "$description": "Popuplar ratios are: 1.067 Minor Second, 1.125 Major Second, 1.200 Minor Third, 1.250 Major Third, 1.333 Perfect Fourth, 1.414 Augmented Fourth, 1.500 Perfect Fifth, 1.618 Golden Ratio"
          },
          "xxs": {
            "$type": "fontSizes",
            "$value": "{core.font-size.typescale.xs} / {core.font-size.typescale.scale}"
          },
          "xs": {
            "$type": "fontSizes",
            "$value": "{core.font-size.typescale.sm} / {core.font-size.typescale.scale}"
          },
          "sm": {
            "$type": "fontSizes",
            "$value": "{core.font-size.REM} / {core.font-size.typescale.scale}"
          },
          "md": {
            "$type": "fontSizes",
            "$value": "{core.font-size.REM}"
          },
          "lg": {
            "$type": "fontSizes",
            "$value": "{core.font-size.typescale.md} * {core.font-size.typescale.scale}"
          },
          "xl": {
            "$type": "fontSizes",
            "$value": "{core.font-size.typescale.lg} * {core.font-size.typescale.scale}"
          },
          "xxl": {
            "$type": "fontSizes",
            "$value": "{core.font-size.typescale.xl} * {core.font-size.typescale.scale}"
          }
        }
